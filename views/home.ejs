<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Thewaystar</title>
    <link rel="stylesheet" href="/css/home.css">
</head>

<body>

  <nav id="navbar">
  <div id="main-header">
    <h1>[The Waystar]</h1>
  </div>
  <div id="navs">
    <ul>
      <li><a href="/home">Home</a></li>
      <li><a href="/create-blog">Create</a></li>
      <li><a href="/myaccount">Account</a></li>
    </ul>
  </div>
</nav>

<!--  Navbar Finished -->


  <div id="userDetail">
    <h1 id="username"> Welcome <%= user.username %></h1>

  </div>
  <br>

       <!-- The Event Data -->
<div class="blog-container">

  <% blogData.forEach(blog => { %>
    <div class="blog-card">
      <div class="blog-content">
        <p>Title: <%= blog.blogName %></p>
        <p><%= blog.blogDescription %></p>
      </div>
    </div>
  <% }) %>

</div>








</body>
</html>
